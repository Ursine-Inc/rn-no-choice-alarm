{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "ignoreDeprecations": "6.0",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@/assets/*": ["./assets/*"],
      "@/app/*": ["./app/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/constants/*": ["./constants/*"],
      "@/components/*": ["./components/*"],
      "@/data/*": ["./data/*"],
      "@/themes/*": ["./themes/*"]
    }
  }
}
