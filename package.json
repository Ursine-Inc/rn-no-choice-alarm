{
  "name": "koom-mobile-app",
  "slug": "no-choice-alarm",
  "description": "KooM! - the alarm clock you can't snooze!",
  "main": "expo-router/entry",
  "version": "1.0.0-pre-alpha.2",
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "dev-build": "eas build --platform android --profile development --local",
    "install-dev-build": "OUTPUT_FILE=$(find . -type f -name \"*.apk\" -print0 | xargs -0 ls -t | head -n1) && adb install -r \"$OUTPUT_FILE\"",
    "rebuild": "pnpm dev-build && pnpm install-dev-build && pnpm start:devclient",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "prepare": "husky install",
    "test": "echo 'no tests yet'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "^7.3.10",
